{
  "name": "js-build-boilerplate",
  "version": "1.0.0",
  "description": "A starting point for creating a stand alone vanilla js module with no framework",
  "main": "index.js",
  "scripts": {
    "test": "mocha-webpack --webpack-config webpack.test.js \"test/**/*.test.js\"",
    "test:watch": "npm test -- --watch",
    "prebuild": "rm -rf dist",
    "build": "export NODE_ENV=production; webpack -p",
    "start": "export NODE_ENV=development; webpack-dev-server -d"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.3",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-node-externals": "^1.6.0"
  },
  "keywords": ["javascript", "webpack", "sass"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkadish/Js-Build-Boilerplate.git"
  },
